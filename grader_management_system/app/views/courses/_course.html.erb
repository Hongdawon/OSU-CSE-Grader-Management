<div id="<%= dom_id course %>">
  <p class="course-number">
    <%= course.subject %>
    <%= course.catalogNumber %>
  </p>

  <p class="course-title">
    <%= course.title %>
  </p>

  <p class="course-term">
    <%= course.term %>
  </p>

  <p class="course-campus">
    <%= course.campus %>
  </p>

  <p class="course-description">
    <%= course.description %>
  </p>

  <p class="course-credits">
    <%= course.credits %>
  </p>
</div>
